# create flower with 3 petals using turtle graphics

from ColabTurtle.Turtle import initializeTurtle, color, bgcolor, goto, pendown, forward, left, done

initializeTurtle()

color('pink')
bgcolor('black')
goto(300,250)
pendown()
for i in range(36):
    for i in range(3):
        forward(100)
        left(120)
    left(10)
    





